{
  "expo": {
    "name": "liefer_mobile",
    "slug": "liefer_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "scheme": "liefer_mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/logo-vertical.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B0637"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#1B0637"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.aniskehila22.liefer_mobile"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./node_modules/@expo-google-fonts/roboto"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c94a23cf-ecd5-4b4b-9235-49b71af5347c"
      }
    }
  }
}
